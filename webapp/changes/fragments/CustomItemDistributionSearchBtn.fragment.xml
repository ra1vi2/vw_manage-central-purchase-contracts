<core:FragmentDefinition 
	xmlns:core='sap.ui.core' 
	xmlns='sap.m'
	xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Button id="idItemDistributionSearchHelpBtn" text="{i18n>CustomItemDistributionHelpBtnLabel}" tooltip="{i18n>CustomItemDistributionHelpBtnLabel}" type="Transparent" enabled="{ui>/editable}"
		press=".extension.vwks.nlp.s2p.mm.pctrcentral.manage.ObjectPageExt.onHandleDistributionSearchHelp" customData:btnName="Add"></Button>
	<Button id="idItemDistributionCopyBtn" text="{i18n>CustomItemDistributionCopyLabel}" tooltip="{i18n>CustomItemDistributionCopyLabel}" type="Transparent" enabled="{= ${ui>/editable} &amp;&amp; ${distributionModel>/enableCopy}}"
		press=".extension.vwks.nlp.s2p.mm.pctrcentral.manage.ObjectPageExt.onHandleDistributionSearchHelp" customData:btnName="Copy"></Button>
</core:FragmentDefinition>
